{
    "name": "yii-extension/flash-message-service",
    "type": "library",
    "description": "Flash message service",
    "keywords": [
        "yii3",
        "flash",
        "message",
        "service"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "composer-runtime-api": "^2.0",
        "yiisoft/session": "^1.0"
    },
    "require-dev": {
        "httpsoft/http-message": "^1.0.5",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\Service\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\Service\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
